responses(map) ::= <<
    <div class="bodyTitle">Responses</div>
    <div class="desc">
        $header()$
        $map.entrySet:{entry | $resEntry(entry.key,entry.value)$}$
        $footer()$
    </div>
>>

response(key,res) ::= <<
    <div class="mimeTitle">$key$</div>
    $res(res)$
>>


res(response) ::= <<

    $response.description;format="markdown"$

    $/namedParams/header(false)$
    $/namedParams/namedParamsElemShort("Headers",response.headers,false,"header")$
    $/namedParams/namedParamsBody(response.body,false)$
    $/namedParams/footer()$
>>

header() ::= <<
    <table class="params">
        <thead>
        <tr>
            <th>Code</th>
            <th class="paramDesc">Description</th>
            <th>Headers</th>
            <th>Body</th>
        </tr>
        </thead>
        <tbody>
>>

resEntry(key,value) ::= <<
    <tr>
        <td>$key$</td>
        <td>$value.description;format="markdown"$</td>
        <td>$value.headers.entrySet:{h | $headerParam(h.key,h.value)$}$</td>
        <td>
            $value.body.entrySet:{entry | $body(entry.key,entry.value)$}$
            $if(value.body)$
                <select name="contentType_false" onchange="rd.showBodyWithId(this,this.value)">
                    $value.body.keys:{key | <option>$key$</option>}$
                </select>
                <script>rd.showBodyWithId(document.currentScript,'$first(value.body.keys)$');</script>
            $endif$
        </td>
    </tr>
>>

body(key,value) ::= <<
    <div class="body $key$">
        <pre class="code prettyprint">$value.schema;format="schema,js"$</pre>
        $if(value.example)$ <p>Example:</p><pre class="code prettyprint">$value.example;format="js"$ </pre> $endif$
    </div>
>>

headerParam(key,value) ::= <<
    <p>$key$: $value.description;format="markdown"$</p>
>>

footer() ::= <<
        </tbody>
    </table>
>>

